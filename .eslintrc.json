{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": ["error", { "args": "none" }],
        "max-len": "off",
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
          ],
          "no-plusplus": "off",
          "no-console": 0,
          "import/extensions": "off",
          "import/no-unresolved": "off" 
    }
    
}
